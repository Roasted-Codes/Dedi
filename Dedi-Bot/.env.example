# Discord Bot Token (required)
# Your Discord bot token from Discord Developer Portal
DISCORD_TOKEN=

# Vultr API Key (required)
# Your Vultr API key from Vultr account settings
VULTR_API_KEY=

ADMIN_USER_IDS=

# Vultr Snapshot ID
VULTR_SNAPSHOT_ID=

# Vultr Region (optional)
# Region code for new instances
# See https://www.vultr.com/api/#tag/region for available regions
VULTR_REGION=dfw

# Vultr Plan (optional)
# Instance plan for new instances (default: vc2-1c-1gb - 1 CPU, 1GB RAM)
# See https://www.vultr.com/api/#tag/plans for available plans
VULTR_PLAN=voc-c-4c-8gb-75s-amd

# Vultr Firewall Group ID
VULTR_FIREWALL_ENABLED=true
VULTR_FIREWALL_GROUP_ID=